{
  "name": "gatsby-plugin-elm",
  "version": "0.1.3",
  "description": "Gatsby plugin to handle elm files",
  "main": "index.js",
  "files": [
    "gatsby-node.js",
    "LICENSE.md",
    "index.js"
  ],
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "format": "standard --fix && standard"
  },
  "standard": {
    "ignore": [
      "./gatsby-node.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wking-io/gatsby-plugin-elm.git"
  },
  "keywords": [
    "elm",
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-elm"
  ],
  "author": "Will King <contact@wking.io> (https://www.wking.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wking-io/gatsby-plugin-elm/issues"
  },
  "homepage": "https://github.com/wking-io/gatsby-plugin-elm#readme",
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "babel-preset-gatsby-package": "1.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.21.0",
    "husky": "5.1.3",
    "standard": "16.0.3"
  },
  "dependencies": {
    "elm-hot-webpack-loader": "1.1.8",
    "elm-webpack-loader": "7.0.1"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0"
  }
}
